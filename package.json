{
    "name": "saurfort-launcher",
    "preductname": "SaurFort's Launcher",
    "version": "2.0.9",
    "description": "Launcher Mincraft pour les serveurs de SaurFort",
    "main": "src/app.js",
    "author": {
        "name": "luuxis, SaurFort, Inc.",
        "email": "contact@saurfort.fr",
        "url": "https://github.com/saurfort"
    },
    "url": "http://launcher.saurfort.fr",
    "scripts": {
        "start": "cross-env-shell NODE_ENV=dev electron .",
        "dev": "cross-env-shell DEV_TOOL=open nodemon --exec npm start",
