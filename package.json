{
    "name": "royal-launcher",
    "preductname": "RoyalCreep's Launcher",
    "version": "1.3.6",
    "description": "Launcher Minecraft pour le serveur RoyalCreep's",
    "main": "src/app.js",
    "author": "AuthenTiiq",
    "url": "https://launcher.royalcreeps.fr",
    "engines": {
        "node": "^18.x.x"
